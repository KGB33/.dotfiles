{
  "python.jediEnabled": false,
  "languageserver": {
    "golang": {
      "command": "gopls",
      "rootPatterns": ["go.mod", ".vim/", ".git/", ".hg/"],
      "filetypes": ["go"],
      "initializationOptions": {
        "usePlaceholders": true
      }
    }
  },
  "cSpell.userWords": [
    "GRUVBOX",
    "Wakatime",
    "gopls",
    "morhetz",
    "neoclide",
    "pypandoc",
    "venv"
  ]
}
